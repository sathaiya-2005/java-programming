import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);

        float input1 = obj.nextInt();     
        float input2 = obj.nextInt();    
        char input3 = obj.next().charAt(0);  

        switch (input3) {
            case '+':
                System.out.printf("Addition of two number is %.1f"  ,(input1 + input2) );
                break;
            case '-':
                System.out.printf("Subtraction of two number is %.1f" ,(input1 - input2) );
                break;
            case '*':
                System.out.printf("Multiplication of two number is %.1f" ,(input1 * input2) );
                break;
            case '/':
                System.out.printf("Division of two number is %.1f" ,(input1 * input2) );
                break;
            default:
                System.out.println("Invalid Input");
                break;
        }
    }
}
