import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner obj = new Scanner(System.in); 

        int x1 = obj.nextInt(); 
        int y1 = obj.nextInt(); 
        int x2 = obj.nextInt();  
        int y2 = obj.nextInt();  

        float midX = (float) (x1 + x2) / 2; 
        float midY = (float) (y1 + y2) / 2; 

        System.out.printf("Binoy's house is located at (%.1f,%.1f)", midX, midY); 
        
    }
}
