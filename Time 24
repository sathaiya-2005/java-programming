import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) 
    {
        
        Scanner obj = new Scanner(System.in);
        int hours = obj.nextInt();
        int minutes = obj.nextInt();
        int seconds = obj.nextInt();

        minutes += seconds /60; 
        seconds = seconds %60; 

        hours += minutes /60; 
        minutes = minutes %60;

        System.out.println("Total Number of hours is " + hours);
        System.out.println("Total Number of minutes is " + minutes);
        System.out.println("Total Number of seconds is " + seconds);
    }
}
