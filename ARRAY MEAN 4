import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int[] arr = new int[n];
        
        float sum = 0;
        
        for (int i = 0; i < n; i++) 
        {
            arr[i] = sc.nextInt();
            sum += arr[i];
        }

        float mean = sum / n;
        
        System.out.printf("Array Mean Value is %.2f%n", mean);
    }
}
